{
   "compilerOptions": {
      "target": "ESNext",
      "jsx": "react-jsx",
      "jsxImportSource": "hono/jsx",
      "baseUrl": "./",
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "typeRoots": ["./node_modules/@types"],
      "types": ["node"],
      "paths": {
         "@/*": ["./src/*"]
      },
      "strict": true,
      "outDir": "./dist",
      "skipLibCheck": true
   },
   "tsc-alias": {
      "resolveFullPaths": true
   }
}
